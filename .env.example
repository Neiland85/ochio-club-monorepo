# Archivo de ejemplo para configuración de variables de entorno

# Puerto en el que corre la aplicación
PORT=4000

# URL de la base de datos PostgreSQL
POSTGRES_URL="postgresql://postgres:suwnup-wepby5-forgeN@db.yfwlnjtkzbjaifvucrhi.supabase.co:5432/postgres"

# Nombre de usuario para la base de datos PostgreSQL
POSTGRES_USER="postgres"

# Host de la base de datos PostgreSQL
POSTGRES_HOST="db.<tu-instancia-supabase>.supabase.co"

# Secreto utilizado para firmar y validar JWTs
JWT_SECRET=<tu-secreto-jwt>

# URL de la instancia de Supabase
SUPABASE_URL=https://<tu-url-de-supabase>

# Clave pública para acceso anónimo a Supabase
SUPABASE_ANON_KEY=<tu-clave-anonima>

# Clave de servicio para operaciones administrativas en Supabase
SUPABASE_SERVICE_ROLE_KEY=<tu-clave-de-servicio>

# URL de conexión a la base de datos PostgreSQL para Prisma
POSTGRES_PRISMA_URL="postgres://<usuario>:<contraseña>@<host>:<puerto>/<basededatos>?sslmode=require"

# Contraseña para la base de datos PostgreSQL
POSTGRES_PASSWORD="<tu-contraseña>"

# Nombre de la base de datos PostgreSQL
POSTGRES_DATABASE="postgres"

# URL de la base de datos PostgreSQL (usada por Prisma y Supabase)
DATABASE_URL="postgresql://postgres:suwnup-wepby5-forgeN@db.yfwlnjtkzbjaifvucrhi.supabase.co:5432/postgres"

